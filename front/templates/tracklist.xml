$def with (tracks)

<tracks>
	$for track in tracks:
		<track id="$track.id">
			<tag>
				<artist>$track.tag.artist</artist>
				<album>$track.tag.album</album>
				<title>$track.tag.title</title>
			</tag>
			<file>
				<size>$track.file.size</size>
				<lastModified>$track.file.last_modified.isoformat()</lastModified>
			</file>
			<duration>$track.duration</duration>
			<seen>$track.seen.isoformat()</seen>
			<path>$track.path</path>
		</track>
</tracks>

